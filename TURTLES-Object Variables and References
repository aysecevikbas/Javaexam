public class TurtleTest3
{
  public static void main(String[] args)
  {
      World world = new World(300,300);
      Turtle yertle = new Turtle(world);
      Turtle myrtle = new Turtle(world);
      Turtle kyrtle = new Turtle(world);
      
      yertle.forward();
      yertle.turnLeft();
      yertle.forward();

      myrtle.turnRight();
      myrtle.forward();

      kyrtle.turnLeft();
      kyrtle.forward();
      
      world.show(true);
  }
}
